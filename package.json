{
    "name": "docilis-api",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm:migration:cli": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d src/configs/database/DataSource.ts",
        "typeorm:seed:cli": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js -d src/configs/database/SeedDatasource.ts",
        "migration:run": "npm run typeorm:migration:cli -- migration:run ",
        "migration:revert": "npm run typeorm:migration:cli -- migration:revert ",
        "migration:show": "npm run typeorm:migration:cli -- migration:show",
        "migration:create": "npm run typeorm:migration:cli -- migration:create src/configs/database/migrations/$npm_config_name",
        "migration:generate": "npm run typeorm:migration:cli -- migration:generate src/configs/database/migrations/$npm_config_name",
        "seed:run": "npm run typeorm:seed:cli -- migration:run",
        "seed:revert": "npm run typeorm:seed:cli -- migration:revert",
        "seed:show": "npm run typeorm:seed:cli -- migration:show",
        "seed:create": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js migration:create src/configs/database/seeds/$npm_config_name"
    },
    "dependencies": {
        "@nestjs/common": "^10.4.6",
        "@nestjs/config": "^3.3.0",
        "@nestjs/core": "^10.4.6",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "^2.0.5",
        "@nestjs/passport": "^10.0.3",
        "@nestjs/platform-express": "^10.4.6",
        "@nestjs/swagger": "^8.0.1",
        "@nestjs/typeorm": "^10.0.2",
        "axios": "^1.7.7",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.3",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "pg": "^8.13.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.1",
        "sqlite3": "^5.1.7",
        "swagger-ui-express": "^5.0.1",
        "typeorm": "^0.3.20"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.14.0",
        "@nestjs/cli": "^10.4.7",
        "@nestjs/schematics": "^10.2.3",
        "@nestjs/testing": "^10.4.6",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.8.7",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.13.0",
        "@typescript-eslint/parser": "^8.13.0",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.12.0",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
